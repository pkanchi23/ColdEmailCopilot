{
  "manifest_version": 3,
  "name": "ColdEmailCopilot",
  "version": "1.0.0",
  "description": "AI-powered cold email generation from LinkedIn profiles. Craft personalized outreach emails with GPT-4 and Claude.",
  "author": "Pranav Kanchi",
  "homepage_url": "https://cold-email-copilot.vercel.app",
  "key": "",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "identity"
  ],
  "oauth2": {
    "client_id": "799846906088-n98o6ucqurtqutv53d8p1kefi7vpupr4.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.compose",
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  },
  "host_permissions": [
    "*://*.linkedin.com/*",
    "*://api.openai.com/*",
    "*://api.anthropic.com/*",
    "*://*.vercel.app/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "js": [
        "utils.js",
        "linkedin-selectors.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}